.tab-content {
    width: 100%;
    margin: 30px auto;
}
.testTitle {
    font-size: 18px;
    font-weight: bold;
    text-align: center;
    margin: 5px 0 30px;
}

.testSubTitle {
    margin-left: 20px;
    margin-bottom: 10px;
    font-size: 16px;
    font-weight: bold
}
.questionContent {
    left: 68px;
    top: 50px;
}

.questionDescrib {
    margin-left: 20px;
    font-size: 14px;
    font-weight: bold;
}
.questionChoses {
    display: flex;
    align-items: center;
    height: 28px;
    margin: 5px 0;
    cursor: pointer;
}

.radioclass {
    display: inline-block;
    width: 18px;
    height: 18px;
    opacity: 0;
    cursor: pointer;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter: alpha(opacity=0);
}

.simbleBox {
    display: inline-block;
    width: 18px;
    height: 18px;
    margin-right: 5px;
    cursor: pointer;
    background-image: url("../assets/images/serviceToolImg/默认选框.png");
}
.testLabel {
    cursor: pointer;
    display: inline-block;
    width: calc(100% - 41px);
}
input[type=radio]:checked + label {
    background-image: url("../assets/images/serviceToolImg/black-nike.png");
}
