

* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#iep {
    width: 1280px;
    height: 720px;
    position: relative;
}
.contentWrap{
    width: 1280px;
    height: 626px;
    position: absolute;
    top: 94px;
    border: 1px solid #cccccc;
}

.contentNav {
    width: 1280px;
    height: 42px;
    position: absolute;
    border-bottom: 1px solid #cccccc;
}

.contentBox {
    width: 1280px;
    height: 584px;
    top: 42px;
    position: absolute;
    border-bottom: 1px solid #cccccc;
}

.slidebarOne {
    width: 94px;
    height: 584px;
    position: absolute;
    border-right: 1px solid #cccccc;
    background-color: #EEEFF0;
}

.contentOne {
    width: 1186px;
    height: 584px;
    position: absolute;
    left: 94px;
}

.slidebarTwo {
    width: 106px;
    height: 584px;
    position: absolute;
}

.contentTwo {
    width: 1080px;
    height: 584px;
    position: absolute;
    left: 106px;
}
.center-content {
    width: 100%;
    height: 584px;
    padding: 5px 20px;
}

.toolArea {
    width: 100%;
    height: 40px;
    display: flex;
    justify-content: flex-end;
    margin-bottom: 4px;
}
.colorTable {
    width: 100%;
    height: 530px;
}
.submitBox{
    width: 100px;
    height: 60px;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    padding-right: 20px;
    position: fixed;
    top: 660px;
    left: calc(50% + 300px);
}
.narrowSubmitBox {
    top: 85%;
}
